<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/malikhollins/gift-libraries</PackageProjectUrl>
    <RepositoryUrl>https://github.com/malikhollins/gift-libraries</RepositoryUrl>
    <PackageId>mhollins.$(AssemblyName)</PackageId>
  </PropertyGroup>

</Project>
